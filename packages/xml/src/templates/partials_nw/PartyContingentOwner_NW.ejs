<%_ if(acord?.PO?.contingentowneryesno=="Yes") { _%>
<Party id="ContingentOwner_1">
  <PartyTypeCode tc="1">Person</PartyTypeCode>
  <Person>
    <Prefix><%=acord?.CO?.Prefix%></Prefix>
    <FirstName><%=acord?.CO?.FirstName%></FirstName>
    <MiddleName><%=acord?.CO?.MiddleInitial%></MiddleName>
    <LastName><%=acord?.CO?.LastName%></LastName>
    <Suffix><%=acord?.CO?.Suffix%></Suffix>
    <BirthDate><%=acord?.CO?.DateofBirth%></BirthDate>
  </Person>
  <GovtIDTC tc="1">SSN</GovtIDTC>
  <GovtID><%=acord?.CO?.SSN?.replace(/[()\-]/g, '')%></GovtID>
  <Address>
    <AddressTypeCode tc="1">Residence</AddressTypeCode>
    <Line1><%=acord?.CO?.Address%></Line1>
    <Line2><%=acord?.CO?.Address%></Line2>
    <City><%=acord?.CO?.City%></City>
    <AddressState><%=acord?.CO?.State%></AddressState>
    <AddressStateTC tc="<%= await lookups.lookup(types.OLI_LU.DRIVERSLICENSESTATENW, {value: acord?.CO.State}).tc%>">
    <%= await lookups.lookup(types.OLI_LU.DRIVERSLICENSESTATENW, {value: acord?.CO?.State}).key%>
    </AddressStateTC>
    <Zip><%=acord?.CO?.Zip?.replace(/[()\-]/g, '')%></Zip>
  </Address>
  <EmailAddress>
    <AddrLine><%=acord?.CO?.EmailAddress%></AddrLine>
  </EmailAddress>
  <Phone>
    <AreaCode><%=acord?.CO?.PhoneNumber?.substring(0,5)?.replace(/[()\-]/g, '')%></AreaCode>
    <DialNumber><%=acord?.CO?.PhoneNumber?.substring(6)?.replace(/[()\-]/g, '')%></DialNumber>
    <PhoneTypeCode tc="<%=acord?.CO?.PhoneType==='Home'?'1':(acord?.CO?.PhoneType==='Cell'?'12':'3')%>"><%=acord?.CO?.PhoneType%></PhoneTypeCode>
  </Phone>
</Party>
<%_ } _%>